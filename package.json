  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asyncApes/clack.git"
  },
  "keywords": [
    "slack",
    "communication",
    "collaboration",
    "teamChat"
  ],
  "author": "Michelle Carter, Peter Schussheim, Tony Xu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asyncApes/clack/issues"
  },
  "homepage": "https://github.com/asyncApes/clack#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
