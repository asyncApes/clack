{
  "name": "clack",
  "version": "1.0.0",
  "description": "A slack clone with surprise features",
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node src/server/index.js",
    "dev": "NODE_ENV=development webpack-dev-server --inline",
    "lint": "eslint ./src ./webpack.config.js -f table || true",
    "build": "NODE_ENV=production && webpack -p",
    "test": "jest",
    "test-watch": "jest --watch-all --coverage"
